env:
  WINIT_X11_SCALE_FACTOR: "2.0"  

font:
  normal:
    family: monospace
    style: Regular

  bold:
    family: monospace
    style: Bold

  italic:
    family: monospace
    style: Italic

  bold_italic:
    family: monospace
    style: Bold Italic

  size: 5

window:
    padding:
      x: 12
      y: 12

scale_with_dpi: true
